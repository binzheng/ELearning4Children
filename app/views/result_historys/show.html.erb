<%- model_class = ResultHistory -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:testResultFirst) %>:</strong></dt>
  <dd><%= @result_history.testResultFirst %></dd>
  <dt><strong><%= model_class.human_attribute_name(:testResultLast) %>:</strong></dt>
  <dd><%= @result_history.testResultLast %></dd>
  <dt><strong><%= model_class.human_attribute_name(:testStartTime) %>:</strong></dt>
  <dd><%= @result_history.testStartTime %></dd>
  <dt><strong><%= model_class.human_attribute_name(:testEndTime) %>:</strong></dt>
  <dd><%= @result_history.testEndTime %></dd>
  <dt><strong><%= model_class.human_attribute_name(:testTypeId) %>:</strong></dt>
  <dd><%= @result_history.testTypeId %></dd>
  <dt><strong><%= model_class.human_attribute_name(:created_id) %>:</strong></dt>
  <dd><%= @result_history.created_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:updated_id) %>:</strong></dt>
  <dd><%= @result_history.updated_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              result_histories_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_result_history_path(@result_history), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              result_history_path(@result_history),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
